# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your_password
DB_DATABASE=dovenue

# Email Configuration (SMTP)
# For development, you can use Ethereal Email (https://ethereal.email/)
# For production, use Gmail, SendGrid, AWS SES, or another SMTP service

SMTP_HOST=smtp.ethereal.email
SMTP_PORT=587
SMTP_USER=your_ethereal_user@ethereal.email
SMTP_PASS=your_ethereal_password
SMTP_FROM=noreply@dovenue.com

# Application URLs
FRONTEND_URL=http://localhost:3001

# JWT Configuration (if needed)
JWT_SECRET=your_jwt_secret_key_here

# Twilio Configuration (for SMS messaging)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here
TWILIO_PHONE_NUMBER=+12345678900

# Node Environment
NODE_ENV=development
