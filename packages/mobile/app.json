{
  "expo": {
    "name": "Event Planning",
    "slug": "event-planning-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventplanning.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nixonlj.eventplanningapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "eventplanning",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "147b2c0b-54fc-4fe3-a6c0-3fdacaeb2ffc"
      }
    }
  }
}
